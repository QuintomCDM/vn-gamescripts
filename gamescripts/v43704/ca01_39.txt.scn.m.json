[
  {
    "character": "一花",
    "japanese": "日が暮れてきちゃった。"
  },
  {
    "character": "風太郎",
    "japanese": "そうだな……。"
  },
  {
    "character": "一花",
    "japanese": "ここの公園、夕焼けも綺麗だね。\n最後に来てみてよかった。"
  },
  {
    "character": "風太郎",
    "japanese": "……ああ。"
  },
  {
    "character": "一花",
    "japanese": "あー、最後なんて言い方はよくないか。\nなんだろ、今日の締め、みたいな？"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "（一緒にいられる時間はもう終わりだ。\n　一花は今夜、地元に戻っちまう）"
  },
  {
    "character": "風太郎",
    "japanese": "（次に会えるのはいつになるのか……）"
  },
  {
    "character": "一花",
    "japanese": "……本当に、綺麗な夕焼け。"
  },
  {
    "character": "風太郎",
    "japanese": "このまま……時間が止まってくれれば、\nずっと見てられるんだけどな。"
  },
  {
    "character": "一花",
    "japanese": "風太郎……。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "一花",
    "japanese": "そういうわけにはいかないよ。\n私には夢があるんだし。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "一花",
    "japanese": "女優として成長して、もっともっと活躍して、\n東京に来て――"
  },
  {
    "character": "一花",
    "japanese": "君と一緒に暮らす。\nそうすれば、時間を止めたいなんて\n思う必要もなくなるでしょ？"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "すまん、変なこと言っちまった。"
  },
  {
    "character": "一花",
    "japanese": "ううん、いいんだよ。"
  },
  {
    "character": "一花",
    "japanese": "君がどんな私も受け入れてくれたように、\n私もどんな風太郎でも受け入れるから。"
  },
  {
    "character": "風太郎",
    "japanese": "……ああ。"
  },
  {
    "character": "一花",
    "japanese": "さてと。\n名残惜しいけど、そろそろ袴を返さないとね。\n風太郎、行こう。"
  },
  {
    "character": "風太郎",
    "japanese": "一花、待ってくれ。"
  },
  {
    "character": "一花",
    "japanese": "何？"
  },
  {
    "character": "風太郎",
    "japanese": "１つ、話しておきたいことがある。"
  },
  {
    "character": "風太郎",
    "japanese": "俺の呼び方なんだが……\nやっぱり呼び捨てでなく今まで通りで頼む。"
  },
  {
    "character": "一花",
    "japanese": "……なんで？"
  },
  {
    "character": "風太郎",
    "japanese": "お前の将来のためだ。\n些細なことではあるが、\nこういう予防線は必要だと思う。"
  },
  {
    "character": "一花",
    "japanese": "でも……。"
  },
  {
    "character": "風太郎",
    "japanese": "それに、呼び方がどうだろうと\n俺たちの思いには関係ないだろ？\n俺をああ呼ぶのは、お前だけなんだし。"
  },
  {
    "character": "一花",
    "japanese": "！……"
  },
  {
    "character": "一花",
    "japanese": "ふふ、それもそっか。"
  },
  {
    "character": "一花",
    "japanese": "いいよ、今まで通りフータロー君で。"
  },
  {
    "character": "風太郎",
    "japanese": "ああ、それで頼む。"
  },
  {
    "character": "一花",
    "japanese": "でも、いずれは変えたいです！"
  },
  {
    "character": "風太郎",
    "japanese": "いずれ……？"
  },
  {
    "character": "一花",
    "japanese": "私、旦那様を君付けで呼ぶのはヤダなー。"
  },
  {
    "character": "風太郎",
    "japanese": "はっ！？"
  },
  {
    "character": "一花",
    "japanese": "というわけなので、\n正式なプロポーズを受けたら変えるね。"
  },
  {
    "character": "風太郎",
    "japanese": "ぷ、プロポーズ……。"
  },
  {
    "character": "一花",
    "japanese": "お父さんともちゃんと話すんだぞ？\n娘さんを俺にくださいって。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "（あ、あの人に向かって……。\n　想像するだけでゾッとしちまう……）"
  },
  {
    "character": "一花",
    "japanese": "わお、見たことないくらい不安そうな顔だ。"
  },
  {
    "character": "風太郎",
    "japanese": "そりゃ、相手が相手だからな。"
  },
  {
    "character": "一花",
    "japanese": "君なら大丈夫だよ。"
  },
  {
    "character": "風太郎",
    "japanese": "何を根拠に……。"
  },
  {
    "character": "一花",
    "japanese": "根拠ならあるよ？"
  },
  {
    "character": "風太郎",
    "japanese": "は？"
  },
  {
    "character": "一花",
    "japanese": "高校生の頃、\n私たちが付き合ってることを\n社長に直接、報告に行ったでしょ？"
  },
  {
    "character": "一花",
    "japanese": "お父さんにも同じようにぶつかれば大丈夫。\n君の気持ちを君らしく伝えればいいんだよ。"
  },
  {
    "character": "風太郎",
    "japanese": "そうだろうか……。"
  },
  {
    "character": "一花",
    "japanese": "うん、私が保証する。\nあの時の君、すごく格好よかった。"
  },
  {
    "character": "風太郎",
    "japanese": "……わかった。\n今のうちから心の準備をしておく。"
  },
  {
    "character": "一花",
    "japanese": "いつまでも待ってるけど、早めだと嬉しいな。"
  },
  {
    "character": "一花",
    "japanese": "って！\n私がここまで言っちゃうと、\n逆にプロポーズしてるみたいじゃん！"
  },
  {
    "character": "風太郎",
    "japanese": "そ、そうだな。"
  },
  {
    "character": "風太郎",
    "japanese": "（というか、昨日、\n　結婚してくれるかって聞かれてるが、\n　あれは俺を叱りつけてくれたようなもんだし……）"
  },
  {
    "character": "一花",
    "japanese": "ほら、行こう！\nもう暗くなっちゃうよ。"
  },
  {
    "character": "風太郎",
    "japanese": "……プロポーズ、か。"
  },
  {
    "character": "風太郎",
    "japanese": "（一花が言ってた通り、早めがいいよな。\n　いつかは絶対にするんだ、こうなれば\n　先延ばしにはしたくない）"
  },
  {
    "character": "風太郎",
    "japanese": "（とはいえ、親父さんを納得させるには\n　現実的な時期でないとダメだ。\n　そうなるとベストなのは――）"
  },
  {
    "character": "風太郎",
    "japanese": "……よし。"
  },
  {
    "character": "一花",
    "japanese": "フータローくーん？"
  },
  {
    "character": "風太郎",
    "japanese": "ああ、今行く。"
  },
  {
    "character": "風太郎",
    "japanese": "（大学生活をやりきって、\n　次のステップへの足場を固め、\n　一花に恥ずかしくない自分になった時――）"
  },
  {
    "character": "風太郎",
    "japanese": "（一花に、プロポーズするんだ）"
  }
]