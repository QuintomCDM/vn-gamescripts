[
  {
    "character": "五月",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "五月",
    "japanese": "……ふぅ。"
  },
  {
    "character": "風太郎",
    "japanese": "キリのいいところまで解けてるみたいだな。\n今日はここまでにするか？"
  },
  {
    "character": "五月",
    "japanese": "ええ、そうですね。\nもう日も傾いてきましたし。"
  },
  {
    "character": "風太郎",
    "japanese": "んじゃ、帰るか。"
  },
  {
    "character": "五月",
    "japanese": "…………。"
  },
  {
    "character": "五月",
    "japanese": "（今日もタイミングを逃してしまいました。\n　せっかく、２人きりの勉強会だったというのに）"
  },
  {
    "character": "五月",
    "japanese": "（上杉君も何も言わない。\n　私からの答え……\n　気にならない、なんてことはないでしょうし）"
  },
  {
    "character": "五月",
    "japanese": "（あの告白の後、みんなにも相談して\n　怒られたり励まされたりして――）"
  },
  {
    "character": "五月",
    "japanese": "（必死に考え、私なりの結論は出ました。\n　後はそれを彼に伝えるだけ）"
  },
  {
    "character": "五月",
    "japanese": "（なのに……）"
  },
  {
    "character": "風太郎",
    "japanese": "五月？"
  },
  {
    "character": "五月",
    "japanese": "…………。"
  },
  {
    "character": "五月",
    "japanese": "（どう話を切り出せばいいのでしょう。\n　なんの前触れもなく答えを告げるのも、\n　きっと、変ですよね）"
  },
  {
    "character": "五月",
    "japanese": "（そもそも彼は、自分の受験もあるというのに\n　私の勉強に付き合ってくれている）"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "五月",
    "japanese": "（そのような場で恋愛の話をするなんて\n　失礼なのでは？）"
  },
  {
    "character": "五月",
    "japanese": "（あの告白以降も上杉君は普段通りな様子ですし、\n　もしかしたら意図的にその話を避けて――）"
  },
  {
    "character": "風太郎",
    "japanese": "……ったく。"
  },
  {
    "character": "風太郎",
    "japanese": "おい。\n何をぼーっとしてんだ。"
  },
  {
    "character": "五月",
    "japanese": "えっ！？"
  },
  {
    "character": "風太郎",
    "japanese": "帰らないのか？"
  },
  {
    "character": "五月",
    "japanese": "あ、え、はい！\nもちろん帰ります、すぐに支度をしますね！"
  },
  {
    "character": "五月",
    "japanese": "（とにかく、次は適切なタイミングを逃さないように。\n　伝え方を間違えてしまったら、今後の関係に\n　支障が出てしまいかねません！）"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "五月",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "五月",
    "japanese": "（他に人の姿も見当たりませんし、\n　いっそ、ここで伝えてしまっても……）"
  },
  {
    "character": "五月",
    "japanese": "（いえ！　やはりそれはおかしいです！\n　こんなの、前置きもなしにしていい話では\n　ありません！）"
  },
  {
    "character": "五月",
    "japanese": "（となると、何かそこに繋げられるような話題を？\n　ど、どんな話題を振ればいいのですか！？）"
  },
  {
    "character": "風太郎",
    "japanese": "あのさ、五月。"
  },
  {
    "character": "五月",
    "japanese": "は、はい！"
  },
  {
    "character": "風太郎",
    "japanese": "……告白のことなんだが。"
  },
  {
    "character": "五月",
    "japanese": "え……。"
  },
  {
    "character": "風太郎",
    "japanese": "だから、後夜祭の。"
  },
  {
    "character": "五月",
    "japanese": "！……"
  },
  {
    "character": "五月",
    "japanese": "（こ、これは！　チャンスなのでは？\n　上杉君のほうから触れてくれましたし、\n　今なら答えを伝えてもおかしくないですよね！？）"
  },
  {
    "character": "五月",
    "japanese": "そ、そうですよね！\nいい加減に答えを――"
  },
  {
    "character": "風太郎",
    "japanese": "悪い、いったん忘れてくれ。"
  },
  {
    "character": "五月",
    "japanese": "……え？"
  },
  {
    "character": "五月",
    "japanese": "（忘れてくれ？\n　あの告白を、忘れろと？）"
  },
  {
    "character": "風太郎",
    "japanese": "お前さ、後夜祭以降、どこか上の空だろ？\n勉強にも身が入ってないみたいだし。"
  },
  {
    "character": "風太郎",
    "japanese": "今日だって問題集、全然進んでなかったよな。"
  },
  {
    "character": "五月",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "それだけ……その、俺とのことを\n真剣に考えてくれてるんだってことは\nわかってるんだ。"
  },
  {
    "character": "風太郎",
    "japanese": "だが結局、俺がお前を困らせてる。\nこんな状態が続けば、お前だけじゃなくて\n他の奴らに迷惑をかけることにもなりかねない。"
  },
  {
    "character": "五月",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "だから……その……。"
  },
  {
    "character": "五月",
    "japanese": "（ダメです、それ以上は――）"
  },
  {
    "character": "風太郎",
    "japanese": "今は、告白のことは忘れてくれ。\nせめて大学受験が終わるまでは保留で――"
  },
  {
    "character": "五月",
    "japanese": "嫌、です。"
  },
  {
    "character": "風太郎",
    "japanese": "え。"
  },
  {
    "character": "五月",
    "japanese": "嫌ですっ！！\n忘れるなんて嫌！！"
  },
  {
    "character": "風太郎",
    "japanese": "い、五月……？"
  },
  {
    "character": "五月",
    "japanese": "なんでそんなこと言うんですか！\n私の返事、待ってくれないんですか！？"
  },
  {
    "character": "風太郎",
    "japanese": "それは――"
  },
  {
    "character": "五月",
    "japanese": "私の気持ち、聞いてくれないんですかっ！？"
  },
  {
    "character": "風太郎",
    "japanese": "！……"
  },
  {
    "character": "五月",
    "japanese": "わかっています、あなたを待たせたのは私です。\n私がこんなことを言うのはお門違いだって。"
  },
  {
    "character": "五月",
    "japanese": "でも、私なりに考えて、答えを出したんです。"
  },
  {
    "character": "風太郎",
    "japanese": "答えって……。"
  },
  {
    "character": "五月",
    "japanese": "私にとってあなたは、これからも隣にいたい人。\n誰よりも特別な人。"
  },
  {
    "character": "風太郎",
    "japanese": "っ……。"
  },
  {
    "character": "五月",
    "japanese": "だから忘れろなんて言わないで。\n私の気持ち、ちゃんと聞いてください。"
  },
  {
    "character": "五月",
    "japanese": "――私、あなたが好きなんです。"
  }
]