[
  {
    "character": "一花",
    "japanese": "うーん……。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "一花",
    "japanese": "あのー、フータロー君？\nここの答えなんだけど――"
  },
  {
    "character": "風太郎",
    "japanese": "今は卒業試験を想定したテスト中だぞ。\nヒントはなしだ。"
  },
  {
    "character": "一花",
    "japanese": "はぁ～い。"
  },
  {
    "character": "風太郎",
    "japanese": "これまで教えたことをよく思い出せば\n正解できるように作ってある。\n諦めずに頑張って埋めてみろ。"
  },
  {
    "character": "一花",
    "japanese": "わかりました、先生。"
  },
  {
    "character": "一花",
    "japanese": "できたぁー！\nこれで全部埋まった！"
  },
  {
    "character": "風太郎",
    "japanese": "おう、よくやった。"
  },
  {
    "character": "一花",
    "japanese": "うう、疲れたぁ……。"
  },
  {
    "character": "風太郎",
    "japanese": "すぐ採点する。\n終わったら忘れないうちに復習するぞ。"
  },
  {
    "character": "一花",
    "japanese": "ええー？\nちょっと休憩させてよぉ。"
  },
  {
    "character": "風太郎",
    "japanese": "お前、わかってるのか？\n来月の特別試験に落ちたら卒業できないんだぞ？"
  },
  {
    "character": "風太郎",
    "japanese": "休学扱いだったお前に与えられたチャンスなんだ。\n学校側の配慮を無にするんじゃない。"
  },
  {
    "character": "一花",
    "japanese": "それは重々承知してますって。"
  },
  {
    "character": "一花",
    "japanese": "君も、自分の受験だってあるのに面倒見てくれてるし、\nちゃんと応えたいと思ってるよ？"
  },
  {
    "character": "一花",
    "japanese": "でも、さすがにもうクタクタ。\nちょっと休まないと何も頭に入らない。"
  },
  {
    "character": "風太郎",
    "japanese": "……頑張りすぎるのもよくないか。\nわかった、しばらく休憩してろ。"
  },
  {
    "character": "一花",
    "japanese": "やったー！　ありがとフータロー君！\nじゃあ、飲み物用意してくるね。"
  },
  {
    "character": "風太郎",
    "japanese": "ああ、その間に採点しとく。"
  },
  {
    "character": "風太郎",
    "japanese": "ふむ……。"
  },
  {
    "character": "風太郎",
    "japanese": "（よし、完璧とまではいかないが\n　合格ラインには届いてるな）"
  },
  {
    "character": "一花",
    "japanese": "どう？　結構、間違ってる？"
  },
  {
    "character": "風太郎",
    "japanese": "いや、かなり正解できてる。\nこの調子でいけば卒業試験は問題ないな。"
  },
  {
    "character": "一花",
    "japanese": "本当？"
  },
  {
    "character": "風太郎",
    "japanese": "嘘をつく必要があるか？"
  },
  {
    "character": "一花",
    "japanese": "よかったぁー。\n頑張ったかいがあったよ。"
  },
  {
    "character": "風太郎",
    "japanese": "ああ、本当によく頑張ってる。\n仕事と両立させてるのは大したもんだ。"
  },
  {
    "character": "一花",
    "japanese": "あれれ～？\nさっきまであんなに厳しかったのに、\n急に優しいこと言ってくれるんだ。"
  },
  {
    "character": "一花",
    "japanese": "アメとムチってやつ？\nフータロー君も上手になったなー。"
  },
  {
    "character": "風太郎",
    "japanese": "俺は正当な評価をしてるだけだ。\nこういう時は素直に受け取っておけよ。"
  },
  {
    "character": "一花",
    "japanese": "ふふっ、ごめんごめん。\nはいこれ、お詫び。"
  },
  {
    "character": "風太郎",
    "japanese": "これ、コーヒー……\nじゃないな、やたらと甘い匂いがするが。"
  },
  {
    "character": "一花",
    "japanese": "ホットチョコレートだよ。\n溶かしたチョコに牛乳を混ぜたやつ。"
  },
  {
    "character": "風太郎",
    "japanese": "へえ、飲むチョコレートってことか。\n初めてかもしれん。"
  },
  {
    "character": "一花",
    "japanese": "ちょっと熱いから気をつけてね。"
  },
  {
    "character": "風太郎",
    "japanese": "おう。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "思ったより甘くなくて飲みやすいな。"
  },
  {
    "character": "一花",
    "japanese": "他には？"
  },
  {
    "character": "風太郎",
    "japanese": "他に？\n……えーと、美味い。"
  },
  {
    "character": "一花",
    "japanese": "うんうん？"
  },
  {
    "character": "風太郎",
    "japanese": "か、体が温まる。"
  },
  {
    "character": "一花",
    "japanese": "はぁ……。\n気がつかないかぁ。"
  },
  {
    "character": "一花",
    "japanese": "ま、仕方ないよね。\n今日は１４日じゃないし。"
  },
  {
    "character": "風太郎",
    "japanese": "１４日……。"
  },
  {
    "character": "風太郎",
    "japanese": "……バレンタインか。"
  },
  {
    "character": "一花",
    "japanese": "当日は撮影があるから先に渡しておきたかったんだ。\nちょうどいいし、このタイミングでどうかなって。"
  },
  {
    "character": "風太郎",
    "japanese": "そ、そういうことか。"
  },
  {
    "character": "一花",
    "japanese": "ハッピーバレンタイン、フータロー君。"
  },
  {
    "character": "風太郎",
    "japanese": "お、おう。\nこういう時って、\nありがたくいただきます、でいいのか？"
  },
  {
    "character": "風太郎",
    "japanese": "あ、いや、もう飲んじまってるし、\nいただきますってのもおかしいよな！？"
  },
  {
    "character": "一花",
    "japanese": "ふふっ、『素直に受け取っておけよ』。"
  },
  {
    "character": "風太郎",
    "japanese": "うっ……。"
  },
  {
    "character": "風太郎",
    "japanese": "あ、ありがとう。"
  },
  {
    "character": "一花",
    "japanese": "どういたしまして。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "（バレンタインのチョコだと思うと、\n　途端にひとくち飲むのも緊張しちまうんだが……）"
  },
  {
    "character": "一花",
    "japanese": "じゃあ、バレンタインを先取りってことで\n今日はこのまま恋人の時間にしちゃう？"
  },
  {
    "character": "風太郎",
    "japanese": "は？"
  },
  {
    "character": "一花",
    "japanese": "みんなも出かけてて２人きりだし、\n勉強はやめにして――"
  },
  {
    "character": "風太郎",
    "japanese": "却下だ。"
  },
  {
    "character": "一花",
    "japanese": "ええー？\n普通、今の流れでそうなる？"
  },
  {
    "character": "風太郎",
    "japanese": "ちゃんと卒業するんだろ？\n合格ラインまで学力は上がってるが、油断は禁物だ！"
  },
  {
    "character": "一花",
    "japanese": "はぁーい。"
  },
  {
    "character": "風太郎",
    "japanese": "これを飲み終わったら、\nさっきのテストの復習をするぞ。"
  },
  {
    "character": "一花",
    "japanese": "じゃ、できるだけゆっくり飲もうかな？"
  },
  {
    "character": "風太郎",
    "japanese": "おい。"
  },
  {
    "character": "一花",
    "japanese": "冗談だよ。\n適度に休んで勉強しまーす。"
  },
  {
    "character": "風太郎",
    "japanese": "そうしてくれ。"
  },
  {
    "character": "一花",
    "japanese": "…………。"
  },
  {
    "character": "一花",
    "japanese": "恋人らしい時間じゃなくても、\n君を独り占めしてるんだから。\n今はそれで充分。"
  },
  {
    "character": "風太郎",
    "japanese": "ん？　なんか言ったか？"
  },
  {
    "character": "一花",
    "japanese": "ううん、なんでも。"
  }
]