[
  {
    "character": "風太郎",
    "japanese": "ん？　誰だ？"
  },
  {
    "character": "三玖",
    "japanese": "フータロー、入ってもいい？"
  },
  {
    "character": "風太郎",
    "japanese": "三玖か。\nああ、構わないぞ。"
  },
  {
    "character": "三玖",
    "japanese": "お邪魔します。"
  },
  {
    "character": "風太郎",
    "japanese": "あれ？\nお前、変装はどうした。"
  },
  {
    "character": "三玖",
    "japanese": "この時間なら、おじいちゃんはもう寝てるから。\nだから大丈夫。"
  },
  {
    "character": "風太郎",
    "japanese": "ああ、そういうことか。\nで、どうした？"
  },
  {
    "character": "三玖",
    "japanese": "なんだか寝つけなくて……\n少し散歩に行こうと思ってるんだけど、いい？"
  },
  {
    "character": "三玖",
    "japanese": "夜だろうが遠慮しないで俺を呼べって、\n言ってくれたよね。"
  },
  {
    "character": "風太郎",
    "japanese": "ああ、もちろん付き合ってやる。\nすぐに着替えるから玄関で待っててくれ。"
  },
  {
    "character": "三玖",
    "japanese": "うん、わかった。"
  },
  {
    "character": "風太郎",
    "japanese": "こうして夜に散歩してると、夏の鎌倉を思い出すな。"
  },
  {
    "character": "三玖",
    "japanese": "うん。\n眠れなくて海辺を歩いたよね。"
  },
  {
    "character": "三玖",
    "japanese": "あの時は夜風が気持ちよかったけど、\n今はもう秋だから、上着がないとちょっと寒いかも。"
  },
  {
    "character": "風太郎",
    "japanese": "ああ。\n少し歩いたら戻ったほうがよさそうだ。"
  },
  {
    "character": "風太郎",
    "japanese": "……それにしても。"
  },
  {
    "character": "三玖",
    "japanese": "……何？\n私の顔、じっと見て。"
  },
  {
    "character": "風太郎",
    "japanese": "ヘッドホンをしてない三玖にまだ慣れなくてな……。"
  },
  {
    "character": "三玖",
    "japanese": "ふふ、実は私も最初はそうだった。\n自分のことなのにね。"
  },
  {
    "character": "三玖",
    "japanese": "みんなも違和感しかないとか言うし。"
  },
  {
    "character": "風太郎",
    "japanese": "まあ、お前と言えばヘッドホンって\nところがあったからなぁ。"
  },
  {
    "character": "三玖",
    "japanese": "でも、これが今の私だから。\nフータローに渡してから、\n必要だって思ったことは一度もないよ。"
  },
  {
    "character": "風太郎",
    "japanese": "……そうか。"
  },
  {
    "character": "三玖",
    "japanese": "ねえ。\nフータローはここの伝説、覚えてる？"
  },
  {
    "character": "風太郎",
    "japanese": "誓いの鐘か？\nこの鐘を鳴らした男女は永遠に結ばれる、だったよな。"
  },
  {
    "character": "三玖",
    "japanese": "せっかくだし鳴らしたいけど、\nさすがにこの時間は迷惑かな。"
  },
  {
    "character": "風太郎",
    "japanese": "なら、明日の昼間に来るか？"
  },
  {
    "character": "三玖",
    "japanese": "それは……今のままだと余裕なさそう。"
  },
  {
    "character": "風太郎",
    "japanese": "……爺さんのことか。"
  },
  {
    "character": "三玖",
    "japanese": "うん……。\nまだ、どう打ち明けるか方法すら浮かんでないから。"
  },
  {
    "character": "三玖",
    "japanese": "こんなに悩むなら、やっぱり\n最初から変装しないで行けばよかったかな。"
  },
  {
    "character": "風太郎",
    "japanese": "（確かに、今の爺さん相手なら\n　それがよかったんだろうが……）"
  },
  {
    "character": "風太郎",
    "japanese": "お前たちが、爺さんを気遣って\nそうしなかったのは間違ってないだろ。"
  },
  {
    "character": "三玖",
    "japanese": "うん……。"
  },
  {
    "character": "風太郎",
    "japanese": "しかし、相変わらず\nお前らに同じ格好をされると混乱するな。\n久々の感覚だ。"
  },
  {
    "character": "三玖",
    "japanese": "…………。"
  },
  {
    "character": "三玖",
    "japanese": "フータロー。"
  },
  {
    "character": "風太郎",
    "japanese": "なんだ？"
  },
  {
    "character": "三玖",
    "japanese": "もし今、五つ子ゲームをしたら\n私たちを見分けられる？"
  },
  {
    "character": "風太郎",
    "japanese": "！……"
  },
  {
    "character": "三玖",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "（今、五つ子ゲームをしたら……）"
  },
  {
    "character": "風太郎",
    "japanese": "三玖のことは、わかる。"
  },
  {
    "character": "三玖",
    "japanese": "……！"
  },
  {
    "character": "風太郎",
    "japanese": "前に見破ったからとかそういうわけじゃなく、\n今の、お前との絆があれば大丈夫というか……。"
  },
  {
    "character": "三玖",
    "japanese": "……そっか。"
  },
  {
    "character": "風太郎",
    "japanese": "おう……。"
  },
  {
    "character": "風太郎",
    "japanese": "その、この際だから言っちまうが、\n実はあの時には、お前の気持ちに薄々勘づいてたんだ。"
  },
  {
    "character": "三玖",
    "japanese": "……そうだったんだ。"
  },
  {
    "character": "風太郎",
    "japanese": "う、薄々だからな？\n確信があったわけじゃない。"
  },
  {
    "character": "風太郎",
    "japanese": "（だが、今にして思うと\n　あの変装を見破れたのは……\n　そういうわけだったってことか）"
  },
  {
    "character": "風太郎",
    "japanese": "ちょっ！？\nみ、三玖！？"
  },
  {
    "character": "三玖",
    "japanese": "その頃から、フータローが\n私を意識してくれてたなら――"
  },
  {
    "character": "風太郎",
    "japanese": "お、おい、何を……。"
  },
  {
    "character": "三玖",
    "japanese": "もっと、こんな風に\n迫ってみればよかったかな？"
  },
  {
    "character": "風太郎",
    "japanese": "（まさか、ここで！？）"
  },
  {
    "character": "三玖",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "三玖",
    "japanese": "でも、やっぱりあの時じゃなくてよかった。"
  },
  {
    "character": "風太郎",
    "japanese": "え……？"
  },
  {
    "character": "三玖",
    "japanese": "あの時の私は、フータローに\n好きになってもらえるような私じゃなかった。"
  },
  {
    "character": "三玖",
    "japanese": "私を好きになってもらえる何かを\nまだ見つけてなかったから。"
  },
  {
    "character": "風太郎",
    "japanese": "三玖……。"
  },
  {
    "character": "三玖",
    "japanese": "あれから、たくさん頑張って……今の私がある。"
  },
  {
    "character": "三玖",
    "japanese": "フータローの隣にいてもいいんだって、\n胸を張って言える私になれたんだ。"
  }
]