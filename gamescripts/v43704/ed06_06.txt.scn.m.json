[
  {
    "character": "五月",
    "japanese": "んんー、お正月らしい空気！\nやっぱり初詣といったらこの感じだよね。"
  },
  {
    "character": "二乃",
    "japanese": "それって屋台の香りってこと？"
  },
  {
    "character": "五月",
    "japanese": "違うから！\n人の雰囲気とかそういう話！"
  },
  {
    "character": "二乃",
    "japanese": "冗談よ、冗談。"
  },
  {
    "character": "五月",
    "japanese": "もう……。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "（こうやって見てる分には、\n　五月もいつも通りだが――）"
  },
  {
    "character": "一花",
    "japanese": "フータロー君。"
  },
  {
    "character": "風太郎",
    "japanese": "ん、どうした？"
  },
  {
    "character": "一花",
    "japanese": "単刀直入に聞いちゃうけど、君、何かやらかした？"
  },
  {
    "character": "風太郎",
    "japanese": "え……。"
  },
  {
    "character": "風太郎",
    "japanese": "（もしかして……バレてるのか？）"
  },
  {
    "character": "三玖",
    "japanese": "図星って顔してる。"
  },
  {
    "character": "風太郎",
    "japanese": "な、なんでそう思ったんだ。"
  },
  {
    "character": "四葉",
    "japanese": "なんと、五月が――"
  },
  {
    "character": "四葉",
    "japanese": "お父さんとデートしてたんです！"
  },
  {
    "character": "風太郎",
    "japanese": "は？"
  },
  {
    "character": "風太郎",
    "japanese": "（親父さんと？\n　それが俺のやらかしにどういう関係が――）"
  },
  {
    "character": "三玖",
    "japanese": "何か落ち込むことがあったら、\nお父さんに慰めてもらう約束だったんだって。"
  },
  {
    "character": "一花",
    "japanese": "それが、クリスマスの後くらいだったんだよね。"
  },
  {
    "character": "風太郎",
    "japanese": "あ……。"
  },
  {
    "character": "風太郎",
    "japanese": "（それってつまり……そういうことだよな）"
  },
  {
    "character": "風太郎",
    "japanese": "（俺と五月は、去年のクリスマスを共に過ごした。\n　そこでプロポーズするつもりだったんだが――）"
  },
  {
    "character": "風太郎",
    "japanese": "（できるだけいい雰囲気で、だとか色々と考えすぎて、\n　タイミングを逃したまま終わってしまった）"
  },
  {
    "character": "風太郎",
    "japanese": "（お互いに就職も決まり、大学卒業間近……。\n　そんな状況なんだ、きっと五月も\n　プロポーズを期待してたんだろう）"
  },
  {
    "character": "風太郎",
    "japanese": "（それなのに肩透かしを食らって……）"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "三玖",
    "japanese": "今度は心当たりがあるって顔。"
  },
  {
    "character": "四葉",
    "japanese": "素直に白状したほうが楽になれますよー？"
  },
  {
    "character": "風太郎",
    "japanese": "……俺から五月に伝えられなかったことがある。"
  },
  {
    "character": "一花",
    "japanese": "あー……。\nやらかしたんじゃなくて、\n何もやらかさなかったわけだ。"
  },
  {
    "character": "風太郎",
    "japanese": "返す言葉もない……。"
  },
  {
    "character": "三玖",
    "japanese": "五月を悲しませるなんて、\nお父さんの逆鱗に触れたかも。"
  },
  {
    "character": "四葉",
    "japanese": "あはは……。\n怒ってないとは言い切れないよね。"
  },
  {
    "character": "風太郎",
    "japanese": "だとしたら当然、受け入れる。\nだが、それよりも……。"
  },
  {
    "character": "風太郎",
    "japanese": "（五月の期待を裏切ってしまったこと、\n　悲しませてしまったことが何よりも……）"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "（あの時、変に考えすぎずに\n　プロポーズができていれば……）"
  },
  {
    "character": "四葉",
    "japanese": "上杉さん、落ち込んでるね。"
  },
  {
    "character": "三玖",
    "japanese": "わかりやすすぎる。"
  },
  {
    "character": "一花",
    "japanese": "やれやれ、仕方ないなぁ。"
  },
  {
    "character": "一花",
    "japanese": "ほら、フータロー君！\nそんなに落ち込んでても仕方ないでしょ？"
  },
  {
    "character": "一花",
    "japanese": "チャンスは一度だけじゃない。\n諦めずにうまくいくまでチャレンジしないと。"
  },
  {
    "character": "風太郎",
    "japanese": "一花……。"
  },
  {
    "character": "三玖",
    "japanese": "新年なんだから、気分を入れ替えるべきだよ。"
  },
  {
    "character": "四葉",
    "japanese": "そうですよ？\n今年はまだ始まったばかりです。"
  },
  {
    "character": "風太郎",
    "japanese": "ああ、ありがとう。"
  },
  {
    "character": "一花",
    "japanese": "とはいえ、同じことを繰り返すようだったら、\nいよいよお父さんにやられるかもね。"
  },
  {
    "character": "風太郎",
    "japanese": "！……\nいらんプレッシャーを……。"
  },
  {
    "character": "三玖",
    "japanese": "そうなったら私たちだって黙ってない。"
  },
  {
    "character": "四葉",
    "japanese": "たとえ上杉さんが相手でも優しくできませんよ～？"
  },
  {
    "character": "風太郎",
    "japanese": "（そうだ、俺には落ち込んでる暇も資格もない。\n　待ってくれている五月のためにも、\n　次こそは……！）"
  },
  {
    "character": "一花",
    "japanese": "さてと。\nお参りもすんだことだし、私はそろそろ時間かな。\n仕事もあるし、ここで。"
  },
  {
    "character": "一花",
    "japanese": "頑張ってね、フータロー君。"
  },
  {
    "character": "風太郎",
    "japanese": "ああ。\nお前も仕事、頑張れよ。"
  },
  {
    "character": "四葉",
    "japanese": "ここでお別れでいいの？\n駅まで送るよ？"
  },
  {
    "character": "一花",
    "japanese": "いいっていいって。\nせっかく神社まで来たんだから、\nみんなはお正月気分を満喫しておきなよ。"
  },
  {
    "character": "五月",
    "japanese": "じゃあ、そうさせてもらうね。"
  },
  {
    "character": "三玖",
    "japanese": "一花、気をつけて。"
  },
  {
    "character": "二乃",
    "japanese": "仕事のしすぎで体を壊さないように。"
  },
  {
    "character": "一花",
    "japanese": "うん、ありがと。\nじゃね～！"
  },
  {
    "character": "四葉",
    "japanese": "私たちはどうしよう？\n屋台とか覗いてみる？"
  },
  {
    "character": "五月",
    "japanese": "私は甘酒を頂いてこようかな。\n参拝客に振る舞ってるのを見かけたから。"
  },
  {
    "character": "二乃",
    "japanese": "私たちは、お店に置く御札を買いに行ってくるわね。"
  },
  {
    "character": "三玖",
    "japanese": "商売繁盛と火災除けと、\nあとは織田信長ゆかりのお守りも買ってこないと。"
  },
  {
    "character": "四葉",
    "japanese": "あっ！\nなら私も行こうかな。\n信長さんのお守り、気になるし。"
  },
  {
    "character": "二乃",
    "japanese": "じゃ、一緒に行きましょ。"
  },
  {
    "character": "三玖",
    "japanese": "というわけだから。\n甘酒は２人で行ってきて。"
  },
  {
    "character": "四葉",
    "japanese": "では、また後でー！"
  },
  {
    "character": "風太郎",
    "japanese": "（これは……\n　俺と五月に気を遣ってくれたってことだろうな）"
  },
  {
    "character": "五月",
    "japanese": "ふう……。\n甘酒で体も温まったし、きしめんも美味しかったね。"
  },
  {
    "character": "風太郎",
    "japanese": "境内にきしめん屋があるなんて知らなかったな。"
  },
  {
    "character": "風太郎",
    "japanese": "（……やっぱり五月はいつも通りだよな）"
  },
  {
    "character": "風太郎",
    "japanese": "（いや、パッと見はそうだとしても、\n　内心では相当気にしてたり――）"
  },
  {
    "character": "五月",
    "japanese": "風太郎？\nどうしたの？"
  },
  {
    "character": "風太郎",
    "japanese": "ああ、いや、その……\nお前はいつも通りだなと思ってさ。"
  },
  {
    "character": "五月",
    "japanese": "それって食べ物の話ばかりってこと？\nそれとも――"
  },
  {
    "character": "五月",
    "japanese": "私がいつも通りなこと自体が気になる？"
  },
  {
    "character": "風太郎",
    "japanese": "うっ……。"
  },
  {
    "character": "五月",
    "japanese": "気になるんだ。"
  },
  {
    "character": "風太郎",
    "japanese": "……気になる。"
  },
  {
    "character": "五月",
    "japanese": "…………。"
  },
  {
    "character": "五月",
    "japanese": "まあ、正直、風太郎に言いたいことはある。"
  },
  {
    "character": "風太郎",
    "japanese": "だよな。"
  },
  {
    "character": "五月",
    "japanese": "でも今は何も言わない。"
  },
  {
    "character": "風太郎",
    "japanese": "え？"
  },
  {
    "character": "五月",
    "japanese": "私と君の恋の形は、一緒に作っていくものだよね？"
  },
  {
    "character": "風太郎",
    "japanese": "あ……。"
  },
  {
    "character": "五月",
    "japanese": "恋には正解も間違いもない。\nだから今はこれでいいんだと思う。"
  },
  {
    "character": "五月",
    "japanese": "私たちが歩んできて、自然にそうなったのなら……\nきっとこれが私たちなりの形なんだから。"
  },
  {
    "character": "風太郎",
    "japanese": "五月……。"
  },
  {
    "character": "風太郎",
    "japanese": "（これって……待ってくれるってことだよな。\n　俺がプロポーズするのを）"
  },
  {
    "character": "風太郎",
    "japanese": "（焦らなくてもいい、俺たちなりの形で）"
  },
  {
    "character": "風太郎",
    "japanese": "（そうだよな。\n　ありがとう五月――）"
  },
  {
    "character": "五月",
    "japanese": "でも。"
  },
  {
    "character": "風太郎",
    "japanese": "いつっ……！？"
  },
  {
    "character": "五月",
    "japanese": "何も言うつもりはないけど、\nこれくらいはしておきたいかな。"
  },
  {
    "character": "風太郎",
    "japanese": "け、結構痛いんだが！？"
  },
  {
    "character": "五月",
    "japanese": "それはそうだよ、痛くしてるんだから。\nでもまだ余裕そうだし、もう少し……。"
  },
  {
    "character": "風太郎",
    "japanese": "いででっ！？"
  },
  {
    "character": "五月",
    "japanese": "ふふっ、お仕置き。"
  },
  {
    "character": "風太郎",
    "japanese": "…………。"
  },
  {
    "character": "風太郎",
    "japanese": "（今の俺はこれくらい、甘んじて受けるしかない。\n　むしろ、これですませてくれて感謝しかない）"
  },
  {
    "character": "風太郎",
    "japanese": "……ありがとな。"
  },
  {
    "character": "五月",
    "japanese": "つねられながらお礼を言うのって、\nちょっとどうかと思うけど。"
  },
  {
    "character": "風太郎",
    "japanese": "それも……俺たちなりの形ってことにしてくれ。"
  },
  {
    "character": "五月",
    "japanese": "うん、そうだね。"
  },
  {
    "character": "風太郎",
    "japanese": "（五月は待ってくれてる。\n　だが、それに甘えてはいられない）"
  },
  {
    "character": "風太郎",
    "japanese": "（だらだらと先延ばしにはしない。\n　次に会う時こそ、プロポーズするんだ）"
  }
]